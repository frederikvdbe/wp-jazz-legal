{
	"name": "antenna/jazzlegal",
	"type": "project",
	"description": "WordPress theme",
	"authors": [
		{
			"name": "Frederik Van Den Berge",
			"email": "frederik@weareantenna.be",
			"homepage": "https://weareantenna.be"
		}
	],
	"repositories": [
		{
			"type": "composer",
			"url": "https://wpackagist.org"
		},
		{
			"type": "composer",
			"url": "https://connect.advancedcustomfields.com"
		},
		{
			"type": "package",
			"package": {
				"name": "gravityforms/gravityforms",
				"version": "2.9.4",
				"type": "wordpress-plugin",
				"dist": {
					"type": "zip",
					"url": "https://www.gravityhelp.com/wp-content/plugins/gravitymanager/api.php?op=get_plugin&slug=gravityforms&key={%WP_PLUGIN_GF_KEY}"
				},
				"require": {
					"gotoandplay/gravityforms-composer-installer": "^2.3"
				}
			}
		},
		{
			"type": "package",
			"package": {
				"name": "sitepress/wpml-multi-lingual-cms",
				"version": "4.7.2",
				"type": "wordpress-plugin",
				"dist": {
					"type": "zip",
					"url": "https://wpml.org/?download=6088&user_id=343105&subscription_key=9d41ba4663b513fd9bbf52d2131ef1fe&t=1742301284&version=4.7.2"
				}
			}
		},
		{
			"type": "package",
			"package": {
				"name": "sitepress/wpml-string-translation",
				"version": "3.3.2",
				"type": "wordpress-plugin",
				"dist": {
					"type": "zip",
					"url": "https://wpml.org/?download=6092&user_id=343105&subscription_key=9d41ba4663b513fd9bbf52d2131ef1fe&t=1742301284&version=3.3.2"
				}
			}
		},
		{
			"type": "package",
			"package": {
				"name": "sitepress/gravityforms-multilingual",
				"version": "1.8.2",
				"type": "wordpress-plugin",
				"dist": {
					"type": "zip",
					"url": "https://wpml.org/?download=8882&user_id=343105&subscription_key=9d41ba4663b513fd9bbf52d2131ef1fe&t=1742301284&version=1.8.2"
				}
			}
		},
		{
			"type": "package",
			"package": {
				"name": "sitepress/yoast-multilingual",
				"version": "2.1.1",
				"type": "wordpress-plugin",
				"dist": {
					"type": "zip",
					"url": "https://wpml.org/?download=3566177&user_id=343105&subscription_key=9d41ba4663b513fd9bbf52d2131ef1fe&t=1742301284&version=2.1.1"
				}
			}
		}
	],
	"require": {
		"php": ">=8.0",
		"composer/installers": "^1.8",
		"vlucas/phpdotenv": "^5.2",
		"oscarotero/env": "^2.1",
		"roots/bedrock-autoloader": "^1.0",
		"roots/wordpress": "^6.0",
		"roots/wp-config": "1.0.0",
		"roots/wp-password-bcrypt": "1.0.0",

		"wpengine/advanced-custom-fields-pro": "^6.2",

		"gravityforms/gravityforms": "^2.8",

		"wpackagist-plugin/wordpress-seo": "^24.0",
		"wpackagist-plugin/acf-content-analysis-for-yoast-seo":"^3.0",

		"sitepress/wpml-multi-lingual-cms": "^4.4",
		"sitepress/wpml-string-translation": "^3.1",
		"sitepress/gravityforms-multilingual": "^1.6",
		"sitepress/yoast-multilingual": "^2.0",

		"wpackagist-plugin/wp-smushit": "^3.9",
		"wpackagist-plugin/autoptimize":"^3.0.4",
		"wpackagist-plugin/imsanity":"^2.7",
		"wpackagist-plugin/intuitive-custom-post-order":"^3.1",
		"wpackagist-plugin/wordfence":"^8.0",
		"wpackagist-plugin/regenerate-thumbnails": "^3.1",
		"wpackagist-plugin/worker":"^4.9",
		"wpackagist-plugin/wp-mail-smtp":"^4.0"
	},
	"require-dev": {
		"wpackagist-plugin/query-monitor": "^3.6"
	},
	"extra": {
		"installer-paths": {
			"web/app/mu-plugins/{$name}/": ["type:wordpress-muplugin"],
			"web/app/plugins/{$name}/": ["type:wordpress-plugin"],
			"web/app/themes/{$name}/": ["type:wordpress-theme"]
		},
		"wordpress-install-dir": "web/wp"
	},
	"scripts": {
		"post-root-package-install": [
			"php -r \"copy('.env.example', '.env');\""
		],
		"test": [
			"phpcs"
		]
	},
	"config": {
		"platform-check": false,
		"allow-plugins": {
			"pivvenit/acf-pro-installer": true,
			"ffraenz/private-composer-installer": true,
			"gotoandplay/gravityforms-composer-installer": true,
			"composer/installers": true,
			"roots/wordpress-core-installer": true
		}
	}
}
